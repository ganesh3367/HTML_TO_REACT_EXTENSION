{
"name": "html-to-react-converter-ganesh3367",
"displayName": "HTML to React",
"publisher": "ganeshwayal",
"description": "Convert selected HTML/CSS into React JSX (basic)",
"version": "0.0.1",
"repository": {
"type": "git",
"url": "https://github.com/ganesh3367/html-to-react.git"
},
"engines": {
"vscode": "^1.80.0"
},
"categories": ["Other"],
"activationEvents": ["onCommand:html-to-react.convert"],
"main": "./out/extension.js",
"contributes": {
"commands": [
{
"command": "html-to-react.convert",
"title": "HTML to React: Convert Selection"
}
],
"keybindings": [
{
"command": "html-to-react.convert",
"key": "ctrl+alt+r",
"when": "editorTextFocus"
}
]
},
"scripts": {
"vscode:prepublish": "npm run compile",
"compile": "tsc -p ./",
"watch": "tsc -w -p ./",
"pretest": "npm run compile",
"test": "node ./out/test/runTest.js"
},
"devDependencies": {
"@types/vscode": "^1.80.0",
"typescript": "^5.1.6",
"@vscode/test-electron": "^2.5.2"
}
}

