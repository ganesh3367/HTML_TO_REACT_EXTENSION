{
  "name": "html-to-react-js",
  "displayName": "HTML to React (JS)",
  "publisher": "ganeshwayal",
  "description": "Convert selected HTML/CSS into React JSX using JavaScript",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "assets/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:html-to-react.convert"
  ],
  "main": "./src/extension.js",
  "contributes": {
  "commands": [
    {
      "command": "html-to-react.convert",
      "title": "Convert to React Component"
    }
  ],
  "menus": {
    "editor/context": [
      {
        "command": "html-to-react.convert",
        "when": "editorHasSelection",
        "group": "navigation"
      }
    ]
  }
}
,
  "scripts": {
    "start": "code .",
    "package": "vsce package",
    "publish": "vsce publish"
  }
}
