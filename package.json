{
  "name": "html-to-react-js",
  "displayName": "HTML to React (JS)",
  "publisher": "ganeshwayal",
  "description": "Convert selected HTML/CSS into React JSX using JavaScript",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "assets/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:html-to-react.convert"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "html-to-react.convert",
        "title": "HTML to React: Convert Selection"
      }
    ],
    "keybindings": [
      {
        "command": "html-to-react.convert",
        "key": "ctrl+alt+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "start": "code .",
    "package": "vsce package",
    "publish": "vsce publish"
  }
}
